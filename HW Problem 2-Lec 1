import java.util.*;
public class hw2 {
	public static void main(String args[])
	{
		LinkedList<Integer> ni=new LinkedList<Integer>();
		Scanner sc=new Scanner(System.in);
		
		System.out.println("Enter the number of elements you wanna add");
		int n=sc.nextInt();
		
		for(int i=0;i<n;i++)
		{
			int val=sc.nextInt();
			if(val>=1 && val<=50)
			{
				ni.add(val);
			}
			else
			{
				System.out.println("Invalid input");
				i--;
			}
		}
		System.out.println("The input  list is:"+ni);
		
		
		 ni.removeIf(x -> x>25);
		 //its a lambda expression
		 //it mns for everv value x in the list,remove elements which hold x>25

	     System.out.println("Required linked list (values â‰¤ 25): " + ni);
	     sc.close();
	}

}
